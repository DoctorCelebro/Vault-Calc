        <TextBox Height="31" HorizontalAlignment="Left" Margin="157,44,0,0" Name="textBox2" VerticalAlignment="Top" Width="22" TextChanged="textBox2_TextChanged" Text="5" />
        <TextBox Height="31" HorizontalAlignment="Left" Margin="186,43,0,0" Name="textBox3" VerticalAlignment="Top" Width="22" TextChanged="textBox3_TextChanged" Text="6" />
        <TextBox Height="32" HorizontalAlignment="Left" Margin="37,156,0,0" Name="textBox4" VerticalAlignment="Top" Width="102" TextChanged="textBox4_TextChanged" />
    </Grid>
</Window>
